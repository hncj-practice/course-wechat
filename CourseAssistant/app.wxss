/**app.wxss**/
@import "main.wxss";
@import "icon.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.root {
  position: relative;
  margin: 0 !important;
  padding: 0px !important;
}

.line {
  width: 100%;
  height: 1px;
  background: #777;
}